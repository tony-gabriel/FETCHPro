<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="fetchpro_splash">

        <item name="android:textSize">30sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/poppins</item>

    </style>

    <style name="fetchpro_headerLight">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="fetchpro_headerDark">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#663399</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="onboarding_textMain">
        <item name="android:textSize">23sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="onboarding_text">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#989898</item>
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="btnDesignLight">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="btnDesignDark">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#663399</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="welcome_header">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="editText">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="header_textMain">
        <item name="android:textSize">23sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="hint_text">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#989898</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="error_text">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#DF0A0A</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="NoActionBar" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="navText">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#A4000000</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="navTitle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="feedText">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#94C4C4C4</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>



    <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
